      <ST Text="EXTEND := FALSE;&amp;#38;#10;RETRACT := FALSE;&amp;#38;#10;" />
    </Algorithm>
    <Algorithm Name="REQ">
      <ST Text="" />
    </Algorithm>
    <Algorithm Name="FWD">
      <ST Text="EXTEND := TRUE;&amp;#38;#10;RETRACT := FALSE;&amp;#38;#10;" />
    </Algorithm>
    <Algorithm Name="BACK">
      <ST Text="EXTEND := FALSE;&amp;#38;#10;RETRACT := TRUE;&amp;#38;#10;" />
    </Algorithm>
    <Algorithm Name="STOP">
      <ST Text="EXTEND := FALSE;&amp;#38;#10;RETRACT := FALSE;&amp;#38;#10;" />
    </Algorithm>
  </BasicFB>
</FBType>

