      </ECState>
      <ECTransition Source="START" Destination="INIT" x="1240" y="1440" Condition="INIT" />
      <ECTransition Source="INIT" Destination="START" x="1306" y="1893" Condition="1" />
      <ECTransition Source="START" Destination="GO" x="1999" y="1886" Condition="REQ[START]" />
      <ECTransition Source="GO" Destination="S_RETURN" x="2800" y="693" Condition="REQ[Extended]" />
