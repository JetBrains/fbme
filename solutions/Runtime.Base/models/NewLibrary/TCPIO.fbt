<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="TCPIO" Comment="Service Interface Function Block Type" Namespace="Runtime.IoCommon">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2009-11-14" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" Vendor="nxtControl" Product="builtin " Version="1.0" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request event">
        <With Var="QI" />
        <With Var="PORT" />
        <With Var="STARTCHAR" />
        <With Var="ENDCHAR" />
      </Event>
      <Event Name="REQ" Comment="Write data request event">
        <With Var="QI" />
        <With Var="DATAOUT" />
      </Event>
      <Event Name="ACK" Comment="Input data processed acknowledge event">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirmation event">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Write data request confirmation event">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND" Comment="Data read indication event">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="PEERADDR" />
        <With Var="DATAIN" />
      </Event>
      <Event Name="ERRIND" Comment="Error indication event">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="PORT" Type="STRING" Comment="Communication port ([UDP:|TCP:][[dotAddr:]portNo][;dotAddr:portNo])" />
      <VarDeclaration Name="STARTCHAR" Type="USINT" Comment="Record start character (optional)" />
      <VarDeclaration Name="ENDCHAR" Type="USINT" Comment="Record end character (optional)" />
      <VarDeclaration Name="DATAOUT" Type="STRING" Comment="Data to be sent with every REQ event" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Error reason in human readable format" />
      <VarDeclaration Name="PEERADDR" Type="STRING" Comment="Address of the peer which has sent the current input data packet" />
      <VarDeclaration Name="DATAIN" Type="STRING" Comment="Data read with every IND event" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>