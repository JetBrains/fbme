<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="919EC124ADF3EDA2" Name="HASH" Comment="Hash a string" Namespace="Runtime.Standard">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_HASH" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="marumil" Date="24.09.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="CNT;1;1;16" />
    <EventInputs>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="ALGO" />
        <With Var="FORMAT" />
        <With Var="INP${CNT}" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="OUT" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="ALGO" Type="USINT" Comment="Algorithm used for computing the hash">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=SHA-1 = 0}" />
      </VarDeclaration>
      <VarDeclaration Name="FORMAT" Type="USINT" Comment="Output format">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=Hex = 0|Raw = 1}" />
      </VarDeclaration>
      <VarDeclaration Name="INP${CNT}" Type="STRING" Comment="Input of the hash function is the concatenation of all the INP parameters" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="OUT" Type="STRING" Comment="Hash of the input" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="HASH" />
</FBType>