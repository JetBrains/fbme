<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BM_FILE" Namespace="Runtime.Fieldbus">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Runtime.FBType.WeakImplementingType" Value="Runtime.Fieldbus#BM_SIMBUS" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2010-05-06" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="PATH_IN" />
        <With Var="PATH_OUT" />
        <With Var="FORMAT" />
        <With Var="SCHEDULE" />
      </Event>
      <Event Name="CONFIG" Comment="Configure the devices on the bus">
        <With Var="QI" />
      </Event>
      <Event Name="RESTART" Comment="Reset the bus and restart">
        <With Var="QI" />
      </Event>
      <Event Name="REQ" Comment="Bus cycle request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="USED_PATH_IN" />
        <With Var="USED_PATH_OUT" />
      </Event>
      <Event Name="CONFIGO" Comment="Bus configuration confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CNF" Comment="Bus cycle request confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="CYCLE_CNT" />
        <With Var="CYCLE_TIME" />
        <With Var="NRETRIES" />
        <With Var="NMISSED" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Uniquely identifies a bus in a device" />
      <VarDeclaration Name="PATH_IN" Type="STRING" Comment="Path of the file providing the input data" />
      <VarDeclaration Name="PATH_OUT" Type="STRING" Comment="Path of the file the output data will be written to" />
      <VarDeclaration Name="FORMAT" Type="USINT" Comment="Format of the input/output data">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=Raw = 0|AutomationX = 1}" />
      </VarDeclaration>
      <VarDeclaration Name="SCHEDULE" Type="STRING" Comment="If not empty, a comma separated list of seconds the update cycle should be performed at. In this case REQ is not necessary." />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays the current status of the bus" />
      <VarDeclaration Name="ERROR_CODE" Type="DINT" Comment="Error code of the bus" />
      <VarDeclaration Name="ERROR_ARG" Type="DINT" Comment="Additional error information" />
      <VarDeclaration Name="CYCLE_CNT" Type="UDINT" Comment="The number of cycles the bus has executed" />
      <VarDeclaration Name="CYCLE_TIME" Type="UDINT" Comment="Time of the last read/write cycle of the bus" />
      <VarDeclaration Name="NRETRIES" Type="UDINT" Comment="Number of retries after the last error" />
      <VarDeclaration Name="USED_PATH_IN" Type="STRING" Comment="Full path of file used for input process image" />
      <VarDeclaration Name="USED_PATH_OUT" Type="STRING" Comment="Full path of file used for ouput process image" />
      <VarDeclaration Name="NMISSED" Type="UDINT" Comment="Number of cycles missed because the data could not be read/written in time" />
    </OutputVars>
  </InterfaceList>
  <Service />
</FBType>