<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BUSDEVICE" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2008-11-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SLOT_NO" />
        <With Var="CONFIGURATION" />
        <With Var="MODULE_ID" />
        <With Var="NINPUTBITS" />
        <With Var="NOUTPUTBITS" />
        <With Var="ISBITDEVICE" />
        <With Var="FLAGS" />
      </Event>
      <Event Name="REQ" Comment="Normal execution request">
        <With Var="QI" />
      </Event>
      <Event Name="DIAGCLR" Comment="Diagnostics output clear request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DIAGNOSTICS" />
      </Event>
      <Event Name="CNF" Comment="Execution confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="DIAGIND" Comment="Diagnostics information available indication">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DIAGNOSTICS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Identifies the bus the current device is attached to" />
      <VarDeclaration Name="SLOT_NO" Type="UDINT" Comment="Defines the number of the slot on the bus occupied by the current device (0..)" />
      <VarDeclaration Name="CONFIGURATION" Type="STRING" Comment="The manufacturer specific parameter string in hex notation" />
      <VarDeclaration Name="MODULE_ID" Type="UDINT" Comment="Unambigously identifies the type of the device on the bus" />
      <VarDeclaration Name="NINPUTBITS" Type="UINT" Comment="Defines the number of bits the device occupies in the input process image of the bus (0..)" />
      <VarDeclaration Name="NOUTPUTBITS" Type="UINT" Comment="Defines the number of bits the device occupies in the output process image of the bus (0..)" />
      <VarDeclaration Name="ISBITDEVICE" Type="BOOL" Comment="Should be set to true if the devices data in the process image is not necessarily byte aligned" />
      <VarDeclaration Name="FLAGS" Type="DWORD" Comment="Various general operation flags" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays current status" />
      <VarDeclaration Name="DIAGNOSTICS" Type="STRING" Comment="The manufacturer specific device diagnostics string" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>