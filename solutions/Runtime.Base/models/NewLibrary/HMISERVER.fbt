<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="HMISERVER" Comment="HMI Server" Namespace="Runtime.Server">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Management" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1" Function="Communication" Description="HMI Server" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="SM" Date="2007-10-23" />
