<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="CPUTICK" Comment="Basic Function Block Type for taking time between events" Namespace="Runtime.System">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.System" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="02.08.2009" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="REQ" Comment="Measure time from the last REQ or START event">
        <With Var="unit" />
      </Event>
      <Event Name="START" Comment="Capture the current time" />
    </EventInputs>
    <EventOutputs>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="timeDiff" />
        <With Var="timeDiffUnits" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="unit" Type="UDINT" InitialValue="0" Comment="Units for the time difference: 0=seconds, 1=milliseconds, 2=microseconds, 3=nanoseconds">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=Seconds = 0|Milliseconds = 1|Microseconds = 2|Nanoseconds = 3}" />
      </VarDeclaration>
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="timeDiff" Type="TIME" Comment="Time since the last REQ event" />
      <VarDeclaration Name="timeDiffUnits" Type="UDINT" Comment="Time since the last REQ event in units as specified in parameter 'unit'" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="CPUTICK" />
</FBType>