<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="COMMCHANNEL" Comment="Basic Function Block Type" Namespace="Runtime.System">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.System" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="milos" Date="2007-06-14" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="PROTO" />
        <With Var="OWNADDR" />
        <With Var="PEERADDR" />
        <With Var="ID" />
        <With Var="ALIVE" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="CLNTQO" />
        <With Var="CLNTSTATUS" />
        <With Var="SRVRQO" />
        <With Var="SRVRSTATUS" />
      </Event>
      <Event Name="CLNTESTABLISHED" Comment="Client (outgoing) connection established (TCP mode only)">
        <With Var="CLNTSTATUS" />
      </Event>
      <Event Name="CLNTCLOSED" Comment="Client (outgoing) connection closed (TCP mode only)">
        <With Var="CLNTSTATUS" />
      </Event>
      <Event Name="SRVRESTABLISHED" Comment="Server (incoming) connection established">
        <With Var="SRVRSTATUS" />
      </Event>
      <Event Name="SRVRCLOSED" Comment="Server (incoming) connection closed">
        <With Var="SRVRSTATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="PROTO" Type="BYTE" InitialValue="2" Comment="Mode 0: TCP w/keepalive and reconnect, 1: UDP w/buffering, 2: UDP realtime (no buffering, no keepalive)">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=TCP = 0|UDP = 1|UDP RT = 2}" />
      </VarDeclaration>
      <VarDeclaration Name="OWNADDR" Type="STRING" Comment="Own address (IP:port, ignored for PROTO other than TCP KA or UDP KA)" />
      <VarDeclaration Name="PEERADDR" Type="STRING" Comment="Peer address (IP:port)" />
      <VarDeclaration Name="ID" Type="ULINT" Comment="Channel ID" />
      <VarDeclaration Name="ALIVE" Type="UINT" Comment="Keepalive interval [ms]" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="CLNTQO" Type="BOOL" Comment="Client output event qualifier" />
      <VarDeclaration Name="CLNTSTATUS" Type="WSTRING" Comment="Client status" />
      <VarDeclaration Name="SRVRQO" Type="BOOL" Comment="Server output event qualifier" />
      <VarDeclaration Name="SRVRSTATUS" Type="WSTRING" Comment="Server status" />
    </OutputVars>
  </InterfaceList>
  <BasicFB />
</FBType>