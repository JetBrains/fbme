<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="E_DELAYR" Comment="Delayed propagation of an event-cancellable and restartable" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1" Classification="Event processing" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="marumil" Date="24.09.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="START" Comment="Begin Delay">
        <With Var="DT" />
      </Event>
      <Event Name="STOP" Comment="Cancel Delay" />
      <Event Name="RESTART" Comment="Begin A New Delay">
        <With Var="DT" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="EO" Comment="Delayed Event" />
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="DT" Type="TIME" Comment="Delay Time" />
    </InputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="E_DELAYR">
    <ServiceSequence Name="event_delay">
      <ServiceTransaction>
        <InputPrimitive Interface="E_DELAYR" Event="START" Parameters="DT" />
        <OutputPrimitive Interface="E_DELAYR" Event="EO" />
      </ServiceTransaction>
    </ServiceSequence>
    <ServiceSequence Name="delay_canceled">
      <ServiceTransaction>
        <InputPrimitive Interface="E_DELAYR" Event="STOP" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_DELAYR" Event="START" Parameters="DT" />
      </ServiceTransaction>
    </ServiceSequence>
    <ServiceSequence Name="no_multiple_delay">
      <ServiceTransaction>
        <InputPrimitive Interface="E_DELAYR" Event="START" Parameters="DT" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_DELAYR" Event="EO" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_DELAYR" Event="START" Parameters="DT" />
      </ServiceTransaction>
    </ServiceSequence>
  </Service>
</FBType>