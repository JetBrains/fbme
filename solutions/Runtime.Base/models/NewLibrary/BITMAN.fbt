<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="7317935F5D9E39D5" Name="BITMAN" Comment="Bit manipulation" Namespace="Runtime.Standard">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_BITMAN" />
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="20.12.2010" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="CNTIN;1;1;32;CNTOUT;1;1;32" />
    <EventInputs>
      <Event Name="INIT" Comment="Initialization">
        <With Var="OP" />
      </Event>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="IN${CNTIN}" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirmation">
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="OUT${CNTOUT}" />
        <With Var="STATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="IN${CNTIN}" Type="&lt;BOOL,BYTE,WORD,DWORD,LWORD&gt;" Comment="Input bit image, bit 0 is the LSB of IN1" />
      <VarDeclaration Name="OP" Type="STRING" Comment="Recipe to wire the input image to the output one, e.g. 0-7,15-8:i,8:0,24=8:1 (first 8 bits as they are, second 8 in reverse order and inverted, 8 zeros and 8 ones from the 24. bit)." />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="OUT${CNTOUT}" Type="&lt;BOOL,BYTE,WORD,DWORD,LWORD&gt;" Comment="Output bit image, bit 0 is the LSB of OUT1" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Status" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>