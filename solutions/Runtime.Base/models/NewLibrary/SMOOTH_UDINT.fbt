<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="SMOOTH_UDINT" Comment="Basic Function Block Type" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="stano" Date="2007-12-19" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="TYPE" />
      </Event>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="TYPE" />
        <With Var="VALDIFF" />
        <With Var="TIMEDIFF" />
        <With Var="CYCLE" />
        <With Var="IN" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm" />
      <Event Name="CNF" Comment="Execution Confirmation (can be also triggered asynchronously)">
        <With Var="OUT" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="TYPE" Type="BYTE" InitialValue="0" Comment="Smoothing type: 0=none; 1=old/new; 2=value; 3=time; 4=time or value; 5=time and value; 6=deflutter" />
      <VarDeclaration Name="VALDIFF" Type="UDINT" Comment="Difference for the value-based smoothings" />
      <VarDeclaration Name="TIMEDIFF" Type="TIME" InitialValue="T#0ms" Comment="Time difference or deflutter time for the time-based smoothings" />
      <VarDeclaration Name="CYCLE" Type="TIME" InitialValue="T#0ms" Comment="If not null, the value will be send at least this often (but not when the deflutter interval is in effect)" />
      <VarDeclaration Name="IN" Type="UDINT" Comment="Input value" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="OUT" Type="UDINT" Comment="Output value" />
    </OutputVars>
  </InterfaceList>
  <BasicFB />
</FBType>