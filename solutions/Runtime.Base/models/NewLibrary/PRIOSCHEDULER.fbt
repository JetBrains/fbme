<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="6F6BAA7400D855A5" Name="PRIOSCHEDULER" Comment="Event schedule function block" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_PRIOSCHEDULER" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="09.09.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;1;4" />
    <EventInputs>
      <Event Name="INIT" Comment="Initialization event">
        <With Var="QI" />
      </Event>
      <Event Name="SET">
        <With Var="QI" />
        <With Var="FIRSTCHANNEL" />
        <With Var="MAXINITTIME" />
        <With Var="SCHEDULE" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Init done notification">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="SETO" Comment="Set new schedule done notification">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND${I}" Comment="Output state changed indicator">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="VALUE${I}" />
        <With Var="ID${I}" />
        <With Var="NEXTVALUE${I}" />
        <With Var="NEXTTS${I}" />
        <With Var="NEXTISVALID${I}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Initialize event input qualifier" />
      <VarDeclaration Name="FIRSTCHANNEL" Type="UDINT" Comment="Defines the number of the first valid channel which corresponds to CHANNEL0" />
      <VarDeclaration Name="MAXINITTIME" Type="TIME" Comment="Defines the timespan to go back to look for initialization events" />
      <VarDeclaration Name="SCHEDULE" Type="STRING" Comment="Defines a list of schedulable events (min hr day mon wday year ch val id [prio [duration]]; ...)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event status qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Error information" />
      <VarDeclaration Name="VALUE${I}" Type="&lt;BOOL,BYTE,WORD,DWORD,LWORD,STRING,SINT,INT,DINT,LINT,USINT,UINT,ULINT,UDINT,LREAL,REAL,TIME&gt;" Comment="Current Value of channel ${I} + FIRSTCHANNEL" />
      <VarDeclaration Name="ID${I}" Type="STRING" Comment="ID of the currently active schedule" />
      <VarDeclaration Name="NEXTVALUE${I}" Type="VALUE${I}" Comment="Value of the next schedule following to the current one" />
      <VarDeclaration Name="NEXTTS${I}" Type="DT" Comment="Timestamp of activating the next schedule" />
      <VarDeclaration Name="NEXTISVALID${I}" Type="BOOL" Comment="Determines if next value and timestamp are valid. If the flag is cleared there is no next schedule" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="PRIOSCHEDULER" />
</FBType>