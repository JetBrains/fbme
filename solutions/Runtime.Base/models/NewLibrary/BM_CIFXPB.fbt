<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BM_CIFXPB" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Runtime.FBType.WeakImplementingType" Value="Runtime.Fieldbus#BM_SIMBUS" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="0.0" Author="mayehor" Date="21.08.2011" Remarks="template" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="BOARD_NO" />
        <With Var="TIMEOUT" />
        <With Var="RESET_TIMEOUT" />
        <With Var="SIMULATION" />
      </Event>
      <Event Name="CONFIG" Comment="Configure the devices on the bus">
        <With Var="QI" />
      </Event>
      <Event Name="REQ" Comment="Bus cycle request">
        <With Var="QI" />
      </Event>
      <Event Name="DUMP">
        <With Var="DUMP_PDIN_OFF" />
        <With Var="DUMP_PDIN_SIZE" />
        <With Var="DUMP_PDOUT_OFF" />
        <With Var="DUMP_PDOUT_SIZE" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CONFIGO" Comment="Bus configuration confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CNF" Comment="Bus cycle request confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="DUMP_PDIN" />
        <With Var="DUMP_PDOUT" />
      </Event>
      <Event Name="IND">
        <With Var="LIFELIST" />
        <With Var="DIAGNOSTICS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Uniquely identifies a bus in a device" />
      <VarDeclaration Name="BOARD_NO" Type="UINT" Comment="Number of the profibus board which is controlled by this FB" />
      <VarDeclaration Name="TIMEOUT" Type="TIME" Comment="Timeout for a RESET to execute (typically 3 to 10 seconds)" />
      <VarDeclaration Name="RESET_TIMEOUT" Type="TIME" Comment="Timout for the interface card to be up and running after a RESET (typically 10 seconds)" />
      <VarDeclaration Name="SIMULATION" Type="BOOL" Comment="Puts the bus into simulation mode" />
      <VarDeclaration Name="DUMP_PDIN_OFF" Type="DINT" />
      <VarDeclaration Name="DUMP_PDIN_SIZE" Type="DINT" />
      <VarDeclaration Name="DUMP_PDOUT_OFF" Type="DINT" />
      <VarDeclaration Name="DUMP_PDOUT_SIZE" Type="DINT" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays the current status of the bus" />
      <VarDeclaration Name="ERROR_CODE" Type="DINT" Comment="Error code of the bus" />
      <VarDeclaration Name="ERROR_ARG" Type="DINT" Comment="Additional error information" />
      <VarDeclaration Name="LIFELIST" Type="STRING" />
      <VarDeclaration Name="DIAGNOSTICS" Type="STRING" />
      <VarDeclaration Name="DUMP_PDIN" Type="STRING" Comment="The number of cycles the bus has executed" />
      <VarDeclaration Name="DUMP_PDOUT" Type="STRING" />
    </OutputVars>
  </InterfaceList>
  <Service />
</FBType>