<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="EVENTCHAINHEAD" Comment="Head of the EVENTCHAIN list" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1" Classification="Event Processing" Function="Chain of events" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2009-10-07" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Register and sort all the FBs in the named chain..">
        <With Var="NAME" />
        <With Var="INVERT" />
      </Event>
      <Event Name="TRIGGER" Comment="Trigger the chain." />
      <Event Name="TRIGGERNEXT" Comment="Trigger the next FB in the chain. Used internally, leave unconnected." />
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Ack the initialization. Loop to TRIGGER to immediately start the chain." />
      <Event Name="FINISHED" Comment="The whole chain was processed." />
      <Event Name="IND" Comment="Fires after INIT and then if the running state changes">
        <With Var="RUNNING" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="NAME" Type="STRING" Comment="Name of the chain this FB is part of." />
      <VarDeclaration Name="INVERT" Type="BOOL" InitialValue="FALSE" Comment="If FALSE, 0 is the highest prio (first to process), if TRUE, 0 is lowest (last to process)." />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="RUNNING" Type="BOOL" Comment="The event chain is running" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="EVENTCHAINHEAD">
    <ServiceSequence Name="trigger">
      <ServiceTransaction>
        <InputPrimitive Interface="EVENTCHAINHEAD" Event="INIT" Parameters="NAME,INVERT" />
        <OutputPrimitive Interface="EVENTCHAINHEAD" Event="INITO" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="EVENTCHAINHEAD" Event="TRIGGER" />
        <OutputPrimitive Interface="resource" Event="trigger_first" />
      </ServiceTransaction>
    </ServiceSequence>
  </Service>
</FBType>