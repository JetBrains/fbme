<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BM_DLL" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Runtime.FBType.WeakImplementingType" Value="Runtime.Fieldbus#BM_SIMBUS" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2013-05-28" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SIMULATION" />
        <With Var="DLL_NAME" />
        <With Var="OPS_NAME" />
        <With Var="CONFIGURATION" />
      </Event>
      <Event Name="CONFIG" Comment="Configure the devices on the bus">
        <With Var="QI" />
      </Event>
      <Event Name="RESTART" Comment="Reset the bus and restart">
        <With Var="QI" />
      </Event>
      <Event Name="REQ" Comment="Bus cycle request">
        <With Var="QI" />
      </Event>
      <Event Name="UPDATE_SETTINGS" Comment="Update the settings" />
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="DRIVER_NAME" />
        <With Var="DRIVER_VERSION" />
      </Event>
      <Event Name="CONFIGO" Comment="Bus configuration confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CNF" Comment="Bus cycle request confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="CYCLE_CNT" />
        <With Var="CYCLE_TIME" />
        <With Var="NRETRIES" />
      </Event>
      <Event Name="UPDATE_SETTINGS_CNF" Comment="Update the settings confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Uniquely identifies a bus in a device" />
      <VarDeclaration Name="SIMULATION" Type="BOOL" Comment="Puts the bus into simulation mode" />
      <VarDeclaration Name="DLL_NAME" Type="STRING" Comment="Name of the DLL containing the driver" />
      <VarDeclaration Name="OPS_NAME" Type="STRING" Comment="Name of the operation structure inside the driver DLL" />
      <VarDeclaration Name="CONFIGURATION" Type="STRING" Comment="Configuration for the driver DLL. This can be a direct configuration string, name of the configuration file or anything else understood by the driver" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays the current status of the bus" />
      <VarDeclaration Name="ERROR_CODE" Type="DINT" Comment="Error code of the bus" />
      <VarDeclaration Name="ERROR_ARG" Type="DINT" Comment="Additional error information" />
      <VarDeclaration Name="CYCLE_CNT" Type="UDINT" Comment="The number of cycles the bus has executed" />
      <VarDeclaration Name="CYCLE_TIME" Type="UDINT" Comment="Time of the last read/write cycle of the bus" />
      <VarDeclaration Name="NRETRIES" Type="UDINT" Comment="Number of retries after the last error" />
      <VarDeclaration Name="DRIVER_NAME" Type="STRING" Comment="Name of the used driver" />
      <VarDeclaration Name="DRIVER_VERSION" Type="STRING" Comment="Version of the used driver" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>