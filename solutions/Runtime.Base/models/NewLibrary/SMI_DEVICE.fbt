<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="SMI_DEVICE" Comment="Basic Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="Horst Mayer" Date="8/8/2009" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="InterfaceID" />
      </Event>
      <Event Name="UP">
        <With Var="SlaveID" />
        <With Var="MotorID" />
      </Event>
      <Event Name="DOWN">
        <With Var="SlaveID" />
        <With Var="MotorID" />
      </Event>
      <Event Name="STOP">
        <With Var="SlaveID" />
        <With Var="MotorID" />
      </Event>
      <Event Name="SETPOS">
        <With Var="Position" />
      </Event>
      <Event Name="SETTILT">
        <With Var="Tilt" />
      </Event>
      <Event Name="GETSTATUS">
        <With Var="SlaveID" />
        <With Var="MotorID" />
      </Event>
      <Event Name="SETID">
        <With Var="SerialNo" />
        <With Var="MotorID" />
        <With Var="ManuID" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
      </Event>
      <Event Name="RX">
        <With Var="AbsolutPos" />
        <With Var="TiltPos" />
        <With Var="Status" />
        <With Var="CycleCnt" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="InterfaceID" Type="STRING" Comment="Unique identifier of the communication interface to the SMI gateway" />
      <VarDeclaration Name="SlaveID" Type="BYTE" Comment="Address of the SMI gateway" />
      <VarDeclaration Name="MotorID" Type="BYTE" />
      <VarDeclaration Name="Position" Type="UINT" />
      <VarDeclaration Name="Tilt" Type="SINT" />
      <VarDeclaration Name="SerialNo" Type="DWORD" />
      <VarDeclaration Name="ManuID" Type="USINT" Comment="Manufacturer ID" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="AbsolutPos" Type="UINT" />
      <VarDeclaration Name="TiltPos" Type="SINT" />
      <VarDeclaration Name="Status" Type="BYTE" />
      <VarDeclaration Name="CycleCnt" Type="UDINT" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="EnoceanCommDevice" />
</FBType>