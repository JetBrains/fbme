<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="CC9A046F013430E9" Name="SERIALIO" Comment="Service Interface Function Block for reading and writing serial lines" Namespace="Runtime.IoCommon">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2009-11-14" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="COM" />
        <With Var="STARTCHAR" />
        <With Var="ENDCHAR" />
      </Event>
      <Event Name="REQ" Comment="Write data request">
        <With Var="QI" />
        <With Var="DATAOUT" />
      </Event>
      <Event Name="ACK" Comment="Input data processed acknowledgement">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Write data request confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND" Comment="Data read indication">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DATAIN" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="COM" Type="STRING" Comment="Communication channel properties delimited by single SPACE characters: {dev=name|baudrate=bps|bits=width|stopbits=[1|2]|parity=[none|even|odd]}" />
      <VarDeclaration Name="STARTCHAR" Type="USINT" Comment="Record start character (optional)" />
      <VarDeclaration Name="ENDCHAR" Type="USINT" Comment="Record end character (optional)" />
      <VarDeclaration Name="DATAOUT" Type="STRING" Comment="Data to be sent with every REQ event" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Error information" />
      <VarDeclaration Name="DATAIN" Type="STRING" Comment="Data read with every IND event" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>