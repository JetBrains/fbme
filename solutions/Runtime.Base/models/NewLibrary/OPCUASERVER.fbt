<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="OPCUASERVER" Comment="OPC UA Server" Namespace="Runtime.Server">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Service" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1" Function="Communication" Description="OPC UA Server" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="SM" Date="2008-03-21" />
  <CompilerInfo header="package fb.rt.nxtcontrol;" />
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialisation Request">
        <With Var="QI" />
        <With Var="MGR_ID" />
        <With Var="SRVCERT" />
      </Event>
      <Event Name="COMM" Comment="Internally used" />
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialisation Confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier" />
      <VarDeclaration Name="MGR_ID" Type="WSTRING" InitialValue="&quot;0.0.0.0:4840&quot;" Comment="Host and port the OPC UA server listens on" />
      <VarDeclaration Name="SRVCERT" Type="STRING" InitialValue="'uaserver.der'" Comment="Server certificate" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier" />
      <VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="OPCUASERVER" />
</FBType>