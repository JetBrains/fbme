<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="4E42C5B40AFCEA95" Name="ENOCEAN_IN" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="0.0" Author="winkhub" Date="10/20/2015" Remarks="template" />
  <CompilerInfo>
    <Compiler Language="Cpp" Vendor="nxtControl" Product="builtin " Version="1.0" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Service Initialization. Must occur after INIT of Master-InterfaceID. QI=false for de-Init">
        <With Var="QI" />
        <With Var="InterfaceID" />
        <With Var="SensorID" />
        <With Var="MatchBaseID" />
      </Event>
      <Event Name="SETID" Comment="lightweight init">
        <With Var="QI" />
        <With Var="SensorID" />
        <With Var="MatchBaseID" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="RecSensorID" />
      </Event>
      <Event Name="IND" Comment="message received">
        <With Var="QO" />
        <With Var="PkType" />
        <With Var="DataByte0" />
        <With Var="DataByte1" />
        <With Var="DataByte2" />
        <With Var="DataByte3" />
        <With Var="RecSensorID" />
        <With Var="StatusByte" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier">
        <Attribute Name="Configuration.Parameter.DefaultConstant" Value="TRUE" />
      </VarDeclaration>
      <VarDeclaration Name="InterfaceID" Type="STRING" Comment="ENOCEAN_STCEVC_DEV or ENOCEAN_CAB master">
        <Attribute Name="Runtime.IO.ParameterValue" Value="" />
      </VarDeclaration>
      <VarDeclaration Name="SensorID" Type="DWORD" Comment="EnOcean URID of Sender. BaseID, 0=any">
        <Attribute Name="Runtime.IO.ParameterValue" Value="" />
      </VarDeclaration>
      <VarDeclaration Name="MatchBaseID" Type="BOOL" Comment="0=exact match, 1=baseIDmatch (skip right zero bits)">
        <Attribute Name="Configuration.Parameter.DefaultConstant" Value="FALSE" />
      </VarDeclaration>
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier" />
      <VarDeclaration Name="PkType" Type="BYTE" Comment="TYPE/ORG/RORG/CHOICE " />
      <VarDeclaration Name="DataByte0" Type="BYTE" Comment="Input data from resource" />
      <VarDeclaration Name="DataByte1" Type="BYTE" />
      <VarDeclaration Name="DataByte2" Type="BYTE" />
      <VarDeclaration Name="DataByte3" Type="BYTE" Comment="first transmitted data byte, contains RPS or 1BS data" />
      <VarDeclaration Name="RecSensorID" Type="DWORD" Comment="received EnOcean sender ID" />
      <VarDeclaration Name="StatusByte" Type="BYTE" Comment="received status byte" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="ENOCEAN_IN" />
</FBType>