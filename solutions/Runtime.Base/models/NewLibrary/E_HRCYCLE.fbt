<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="E_HRCYCLE" Comment="High resolution periodic generation of an event" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1" Classification="Event Processing" Function="High Resolution Cyclic Event Generation" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2010-09-20" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="START" Comment="Start generating periodical events">
        <With Var="PERIOD" />
        <With Var="PHASE" />
      </Event>
      <Event Name="STOP" Comment="Stop generating periodical events" />
    </EventInputs>
    <EventOutputs>
      <Event Name="EO" Comment="Periodic event at period DT,starting at DT after START" />
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="PERIOD" Type="UDINT" Comment="Period between events in microseconds" />
      <VarDeclaration Name="PHASE" Type="UDINT" Comment="Phase shift in microseconds (has to be less than PERIOD)" />
    </InputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="E_HRCYCLE">
    <ServiceSequence Name="event_cycle">
      <ServiceTransaction>
        <InputPrimitive Interface="resource" Event="timeout" />
        <OutputPrimitive Interface="E_HRCYCLE" Event="EO" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="resource" Event="timeout" />
        <OutputPrimitive Interface="E_HRCYCLE" Event="EO" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_HRCYCLE" Event="START" Parameters="PERIOD,PHASE" />
        <OutputPrimitive Interface="resource" Event="start_cycling" Parameters="PERIOD,PHASE" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="resource" Event="timeout" />
        <OutputPrimitive Interface="E_HRCYCLE" Event="EO" />
      </ServiceTransaction>
    </ServiceSequence>
    <ServiceSequence Name="cycle_canceled">
      <ServiceTransaction>
        <InputPrimitive Interface="E_HRCYCLE" Event="STOP" />
        <OutputPrimitive Interface="resource" Event="stop_cycling" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_HRCYCLE" Event="START" Parameters="PERIOD,PHASE" />
        <OutputPrimitive Interface="resource" Event="start_cycling" Parameters="PERIOD,PHASE" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="resource" Event="timeout" />
        <OutputPrimitive Interface="E_HRCYCLE" Event="EO" />
      </ServiceTransaction>
    </ServiceSequence>
  </Service>
</FBType>