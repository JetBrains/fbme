<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="RTWatchdog" Comment="Watchdog" Namespace="Runtime.Management">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Management" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2011-07-01" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization">
        <With Var="QI" />
        <With Var="ACTION" />
      </Event>
      <Event Name="START" Comment="Activate the watchdog. After starting the RESET event has to be called periodically.">
        <With Var="ACTION" />
      </Event>
      <Event Name="RESET" Comment="Reset the watchdog interval." />
      <Event Name="STOP" Comment="Deactivate the watchdog. If hardware watchdog that cannot be disabled is used, the nxtRT61499F will continue to reset it on its own." />
      <Event Name="TRIGGER" Comment="Immediately perform the watchdog expire action." />
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirmation">
        <With Var="QO" />
      </Event>
      <Event Name="STARTO" Comment="Start confirmation">
        <With Var="EXPIRETIME" />
      </Event>
      <Event Name="RESETO" Comment="Reset confirmation" />
      <Event Name="STOPO" Comment="Stop confirmation" />
      <Event Name="EXPIRED" Comment="Watchdog expired (if Action = Event)" />
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" InitialValue="TRUE" Comment="If FALSE the watchdog has no function" />
      <VarDeclaration Name="ACTION" Type="USINT" InitialValue="1" Comment="Action to perform on expire (default is platform-dependent). If the platform provides a hardware watchdog, only Reboot is able to use it (other types use software one).">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=Default = 0|No action = 1|Event = 2|Log = 3|Exit = 4|Reboot = 5}" />
      </VarDeclaration>
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Init Output Qualifier" />
      <VarDeclaration Name="EXPIRETIME" Type="TIME" Comment="Maximum interval between the RESET events (platform-dependent, at least 1 second)" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="RTWatchdog">
    <ServiceSequence Name="wdexpire">
      <ServiceTransaction>
        <InputPrimitive Interface="RTWatchdog" Event="expire" />
        <OutputPrimitive Interface="RESOURCE" Event="expire action" />
      </ServiceTransaction>
    </ServiceSequence>
  </Service>
</FBType>