<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="8B41E3E0886ADB0B" Name="MODBUSIO" Comment="Service Interface Function Block Type" Namespace="Runtime.IoCommon">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.IoCommon#DYNGEN_MODBUSIO" />
  <Attribute Name="Runtime.MODBUS.Server" Value="TCP:502" />
  <Attribute Name="Runtime.MODBUS.IOAddress" Value="1" />
  <Attribute Name="Runtime.MODBUS.Storage" Value="0" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="1.0" Author="oberwal" Date="2012-02-11" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;0;64;O;1;0;64" />
    <EventInputs>
      <Event Name="INIT" Comment="Service initialization">
        <With Var="QI" />
      </Event>
      <Event Name="LOAD_RETAIN" Comment="Load the retain values (or default if no retain ones present)">
        <With Var="QI" />
        <With Var="default${O}" />
      </Event>
      <Event Name="REQ" Comment="Process memory write request">
        <With Var="QI" />
        <With Var="SD${I}" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Confirmation of initialization complete">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Confirmation of a process memory write request">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND" Comment="Indication of a MODBUS write operation">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${O}" />
      </Event>
      <Event Name="INDCHG" Comment="Indication of a MODBUS write operation with at least one data item changed">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${O}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="SD${I}" Type="&lt;BOOL, INT, UINT, WORD, DINT, UDINT, DWORD, REAL&gt;" Comment="Data to be written to MODBUS process image" />
      <VarDeclaration Name="default${O}" Type="RD${O}" Comment="Defaults for data read from MODBUS process image until first data come or there are retain data" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays current accessor status" />
      <VarDeclaration Name="RD${O}" Type="&lt;BOOL, INT, UINT, WORD, DINT, UDINT, DWORD, REAL&gt;" Comment="Data read from MODBUS process image" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="MODBUSIO" />
</FBType>