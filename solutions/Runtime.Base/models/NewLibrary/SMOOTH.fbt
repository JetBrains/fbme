<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="DE89288BECD15AFE" Name="SMOOTH" Comment="Smooth" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_SMOOTH" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2012-01-01" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="CNT;1;1;16" />
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="TYPE" />
        <With Var="VALCONDITION" />
        <With Var="VALDIFF${CNT}" />
        <With Var="TIMEDIFF" />
        <With Var="CYCLE" />
      </Event>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="IN${CNT}" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm" />
      <Event Name="CNF" Comment="Execution Confirmation (can be also triggered asynchronously)">
        <With Var="OUT${CNT}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="TYPE" Type="BYTE" InitialValue="0" Comment="Smoothing type: 0=none; 1=old/new; 2=value; 3=time; 4=time or value; 5=time and value;">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=None = 0|OldNew = 1|Value = 2|Time = 3|TimeOrValue = 4|TimeAndValue = 5}" />
      </VarDeclaration>
      <VarDeclaration Name="VALCONDITION" Type="BYTE" InitialValue="0" Comment="Condition for value-based smoothing: 0=send if any value differ enough; 1=send if all values differs enough">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=Any = 0|All = 1}" />
      </VarDeclaration>
      <VarDeclaration Name="TIMEDIFF" Type="TIME" InitialValue="T#0ms" Comment="Time difference for the time-based smoothings" />
      <VarDeclaration Name="CYCLE" Type="TIME" InitialValue="T#0ms" Comment="If not null, the value(s) will be sent at least this often" />
      <VarDeclaration Name="VALDIFF${CNT}" Type="IN${CNT}" Comment="Difference for the value-based smoothings" />
      <VarDeclaration Name="IN${CNT}" Type="&lt;ANY&gt;" Comment="Input value" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="OUT${CNT}" Type="IN${CNT}" Comment="INx at the time of CNF" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="SMOOTH" />
</FBType>