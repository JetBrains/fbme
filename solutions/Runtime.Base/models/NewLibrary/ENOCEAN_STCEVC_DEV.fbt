<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="3058FDC7ECF79F4F" Name="ENOCEAN_STCEVC_DEV" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="0.0" Author="winkhub" Date="10/22/2015" Remarks="template" />
  <CompilerInfo>
    <Compiler Language="Cpp" Vendor="nxtControl" Product="builtin " Version="1.0" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Service Initialization">
        <With Var="QI" />
        <With Var="InterfaceID" />
        <With Var="Com" />
        <With Var="GatewayAddress" />
      </Event>
      <Event Name="REQ" Comment="send GatewayCommand">
        <With Var="QI" />
        <With Var="GatewayAddress" />
        <With Var="GatewayCommand" />
      </Event>
      <Event Name="AEP_QUERY" Comment="refresh the AepList and produce an AEP_IND event ">
        <With Var="QI" />
      </Event>
      <Event Name="AEP_ADD" Comment="add a new actuator with AepPath and AepId to the AepList and produce an AEP_IND event">
        <With Var="QI" />
        <With Var="GatewayAddress" />
        <With Var="AepPath" />
        <With Var="AepId" />
      </Event>
      <Event Name="AEP_DEL" Comment="delete actuator AepPath from the AepList and produce an AEP_IND event. AepPath * deletes all, (blank) deletes AepId.">
        <With Var="QI" />
        <With Var="GatewayAddress" />
        <With Var="AepPath" />
        <With Var="AepId" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="INIT confirm">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND" Comment="incoming message from gateway">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="Message" />
      </Event>
      <Event Name="AEP_IND" Comment="AEP_* confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="AepList" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier">
        <Attribute Name="Configuration.Parameter.DefaultConstant" Value="TRUE" />
      </VarDeclaration>
      <VarDeclaration Name="InterfaceID" Type="STRING" Comment="Internal Connection Name">
        <Attribute Name="Runtime.IO.ParameterValue" Value="" />
      </VarDeclaration>
      <VarDeclaration Name="Com" Type="STRING" Comment="Communication channel properties delimited by single SPACE characters: {dev=name baudrate=bps bits=width stopbits=[1|2] parity=[none|even|odd]}  use dev=/dev/ttyAPP0 syntax in Linux">
        <Attribute Name="Configuration.Parameter.DefaultConstant" Value="'dev=COM1 baudrate=9600 bits=8 stopbits=1 parity=odd'" />
        <Attribute Name="Runtime.IO.ParameterValue" Value="" />
      </VarDeclaration>
      <VarDeclaration Name="GatewayAddress" Type="USINT" Comment="Gateway DIP switch 0..63">
        <Attribute Name="Runtime.IO.ParameterValue" Value="" />
      </VarDeclaration>
      <VarDeclaration Name="GatewayCommand" Type="STRING" Comment="send gateway command, hex format, Sync-header, checksum and gateway address is auto filled">
        <Attribute Name="Configuration.Parameter.DefaultConstant" Value="'AB 58 00 00 00 00 00000000 00 00'" />
      </VarDeclaration>
      <VarDeclaration Name="AepPath" Type="STRING" Comment="name for AEP ADD, name for AEP_DEL, * deletes all, '' (empty) deletes AepId" />
      <VarDeclaration Name="AepId" Type="UINT" Comment="Tx-Id for AEP_ADD and AEP_DEL 0..127" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier. True signal OK, False Failure. see STATUS for error message." />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="information and error" />
      <VarDeclaration Name="Message" Type="STRING" Comment="information of RX or TX telegram" />
      <VarDeclaration Name="AepList" Type="STRING" Comment="all registered actuator end points, &lt;apath0&gt;,&lt;aid0&gt;|&lt;apath1&gt;,&lt;aid1&gt;|&lt;apathN&gt;,&lt;aidN&gt;" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="ENOCEAN_STCEVC_DEV" />
</FBType>