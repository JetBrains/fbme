<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BC_MODBUS" Namespace="Runtime.Fieldbus">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2011-06-07" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SLOT_NO" />
        <With Var="CONFIGURATION" />
        <With Var="MODULE_ID" />
        <With Var="FLAGS" />
        <With Var="SLAVE_BUS_ID" />
        <With Var="OPTIONS" />
      </Event>
      <Event Name="REQ" Comment="Normal execution request">
        <With Var="QI" />
      </Event>
      <Event Name="DIAGCLR" Comment="Diagnostics output clear request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DIAGNOSTICS" />
      </Event>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="DIAGIND" Comment="Diagnostics information available indication">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DIAGNOSTICS" />
      </Event>
      <Event Name="IND" Comment="Indication of a MODBUS write operation, new data is available">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="DIAGNOSTICS" />
        <With Var="CYCLE_CNT" />
        <With Var="CYCLE_TIME" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Identifies the bus the current device is attached to" />
      <VarDeclaration Name="SLOT_NO" Type="UDINT" Comment="Defines the number of the slot on the bus occupied by the current device (0..)" />
      <VarDeclaration Name="CONFIGURATION" Type="STRING" Comment="The manufacturer specific parameter string in hex notation" />
      <VarDeclaration Name="MODULE_ID" Type="UDINT" Comment="Unambigously identifies the type of the device on the bus" />
      <VarDeclaration Name="FLAGS" Type="DWORD" Comment="Various general operation flags" />
      <VarDeclaration Name="SLAVE_BUS_ID" Type="STRING" Comment="Defines the name for the slave bus referenced by all slave devices" />
      <VarDeclaration Name="OPTIONS" Type="STRING" Comment="Defines slave bus configurtion options (e.g. MODE=KBUS, ...)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Error information" />
      <VarDeclaration Name="DIAGNOSTICS" Type="STRING" Comment="The manufacturer specific device diagnostics string" />
      <VarDeclaration Name="CYCLE_CNT" Type="UDINT" Comment="The number of cycles the modbus bus coupler has executed" />
      <VarDeclaration Name="CYCLE_TIME" Type="UDINT" Comment="Time of the last read/write cycle of the modbus bus coupler" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>