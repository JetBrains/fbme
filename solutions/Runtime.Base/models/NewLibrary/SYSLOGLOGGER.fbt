<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="SYSLOGLOGGER" Comment="Sends the logged messages to a syslog server" Namespace="Runtime.System">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.System" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="18.12.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="SERVER" />
        <With Var="FACILITY" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" InitialValue="TRUE" Comment="Input event qualifier" />
      <VarDeclaration Name="SERVER" Type="STRING" Comment="Destination host (on supported platforms use &quot;API&quot; to use the syslog API instead of UDP)" />
      <VarDeclaration Name="FACILITY" Type="USINT" InitialValue="16" Comment="Syslog facility as of RFC 3164">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=User-level = 1|System daemons = 3|Local 0 = 16|Local 1 = 17|Local 2 = 18|Local 3 = 19|Local 4 = 20|Local 5 = 21|Local 6 = 22|Local 7 = 23}" />
      </VarDeclaration>
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Status of the initialization" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>