<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="E_TRAIN" Comment="Generation of a finite train of events" Namespace="IEC61499.Standard">
  <Attribute Name="ObsoleteNamespace" Value="IEC61499.Common" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="2008-09-09" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="START" Comment="Event to start scheduled event outputs">
        <With Var="DT" />
        <With Var="N" />
      </Event>
      <Event Name="STOP" Comment="Event to stop scheduled event outputs" />
    </EventInputs>
    <EventOutputs>
      <Event Name="EO" Comment="N events at period DT,starting at DT after START">
        <With Var="CV" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="DT" Type="TIME" Comment="Period between events" />
      <VarDeclaration Name="N" Type="UINT" Comment="Number of events to generate" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="CV" Type="UINT" Comment="EO index(0 N-1)" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="E_TRAIN">
    <ServiceSequence Name="normal_operation" Comment="N=3">
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=1" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=2" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_TRAIN" Event="START" Parameters="DT" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=0" />
      </ServiceTransaction>
    </ServiceSequence>
    <ServiceSequence Name="no_restart" Comment="N=3">
      <ServiceTransaction>
        <InputPrimitive Interface="E_TRAIN" Event="START" Parameters="DT" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=1" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=2" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_TRAIN" Event="START" Parameters="DT" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=0" />
      </ServiceTransaction>
    </ServiceSequence>
    <ServiceSequence Name="cycle_canceled">
      <ServiceTransaction>
        <InputPrimitive Interface="E_TRAIN" Event="STOP" />
      </ServiceTransaction>
      <ServiceTransaction>
        <InputPrimitive Interface="E_TRAIN" Event="START" Parameters="DT" />
      </ServiceTransaction>
      <ServiceTransaction>
        <OutputPrimitive Interface="E_TRAIN" Event="EO" Parameters="CV=0" />
      </ServiceTransaction>
    </ServiceSequence>
  </Service>
</FBType>