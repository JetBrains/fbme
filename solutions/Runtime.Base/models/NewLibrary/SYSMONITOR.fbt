<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="SYSMONITOR" Comment="Standard system monitor" Namespace="Runtime.System">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.System" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-1-D.1" Classification="System" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2008-06-12" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
      </Event>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="QI" />
      </Event>
      <Event Name="SET" Comment="Request Time Setting">
        <With Var="QI" />
        <With Var="CURRTIME" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirmation">
        <With Var="QO" />
        <With Var="ERROR" />
      </Event>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="QO" />
        <With Var="ERROR" />
        <With Var="VERSION" />
        <With Var="SYSTIME" />
        <With Var="CPULOAD" />
        <With Var="HDLFREE" />
        <With Var="HDLUSED" />
        <With Var="MEMFREE" />
        <With Var="MEMUSED" />
        <With Var="MSGSENT" />
        <With Var="MSGRCVD" />
        <With Var="NWBOVFL" />
        <With Var="CYCLEOVFL" />
      </Event>
      <Event Name="SETO" Comment="Time Setting Confirmation">
        <With Var="QO" />
        <With Var="ERROR" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="CURRTIME" Type="STRING" Comment="New date and time to be set (CCYYMMDDhhmmss)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="ERROR" Type="BOOL" Comment="NOT QO, use QO instead" />
      <VarDeclaration Name="VERSION" Type="STRING" Comment="Current forte version identifier" />
      <VarDeclaration Name="SYSTIME" Type="WSTRING" Comment="System time in human readable format" />
      <VarDeclaration Name="CPULOAD" Type="REAL" Comment="CPU usage in load" />
      <VarDeclaration Name="HDLFREE" Type="UDINT" Comment="Number of unused operating system handles" />
      <VarDeclaration Name="HDLUSED" Type="UDINT" Comment="Number of used operating system handles" />
      <VarDeclaration Name="MEMFREE" Type="UDINT" Comment="Physical unallocated memory [KB]" />
      <VarDeclaration Name="MEMUSED" Type="UDINT" Comment="Physical allocated memory [KB]" />
      <VarDeclaration Name="MSGSENT" Type="UDINT" Comment="Average number of messages sent [msgs/s" />
      <VarDeclaration Name="MSGRCVD" Type="UDINT" Comment="Average number of messages received [msgs/s]" />
      <VarDeclaration Name="NWBOVFL" Type="UDINT" Comment="Number of network buffer overflows" />
      <VarDeclaration Name="CYCLEOVFL" Type="UDINT" Comment="Number of missed timer events (cycle overflows)" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="SYSMONITOR" />
</FBType>