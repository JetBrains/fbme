<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="442E8BA737EFFC55" Name="ANAMATH" Comment="Basic Function Block Type For Array Mathematics" Namespace="Runtime.Standard">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_ANAMATH" />
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="02.08.2009" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="i;1;1;1" />
    <EventInputs>
      <Event Name="REQ" Comment="Normal Execution Request">
        <With Var="pv${i}" />
        <With Var="len" />
      </Event>
      <Event Name="RESET" Comment="Reset Internal State Request" />
    </EventInputs>
    <EventOutputs>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="tAvg${i}" />
        <With Var="tMax${i}" />
        <With Var="tMin${i}" />
        <With Var="tDeadT${i}" />
      </Event>
      <Event Name="RESETO" Comment="Internal State Reset Request Confirmation">
        <With Var="tAvg${i}" />
        <With Var="tMax${i}" />
        <With Var="tMin${i}" />
        <With Var="tDeadT${i}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="pv${i}" Type="REAL, LREAL, DINT, UDINT" Comment="Input value" />
      <VarDeclaration Name="len" Type="DINT" Comment="Number of input values" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="tAvg${i}" Type="pv${i}" Comment="Average of the last len input values" />
      <VarDeclaration Name="tMax${i}" Type="pv${i}" Comment="Maximum of the last len input values" />
      <VarDeclaration Name="tMin${i}" Type="pv${i}" Comment="Minimum value of the last len input values" />
      <VarDeclaration Name="tDeadT${i}" Type="pv${i}" Comment="The len last input value" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="ANAMATH" />
</FBType>