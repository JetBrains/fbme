<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="ENOCEANIF_SERIAL" Comment="Basic Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="Horst Mayer" Date="8/8/2009" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="COM" />
        <With Var="Baudrate" />
        <With Var="Parity" />
        <With Var="InterfaceID" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
        <With Var="Status" />
      </Event>
      <Event Name="IND" Comment="Execution Confirmation">
        <With Var="QO" />
        <With Var="Message" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="COM" Type="STRING" Comment="COMx interface qualifier" />
      <VarDeclaration Name="Baudrate" Type="UDINT" InitialValue="9600" Comment="Baudrate of the serial interface">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=9600=9600|19200=19200|38400=38400|115200=115200}" />
      </VarDeclaration>
      <VarDeclaration Name="Parity" Type="USINT" InitialValue="1" Comment="Parity of the serial interface (none = 0, even = 1, odd = 2)">
        <Attribute Name="ParameterEditor" Value="ComboBox{Enums=none=0|even=1|odd=2}" />
      </VarDeclaration>
      <VarDeclaration Name="InterfaceID" Type="STRING" Comment="Uniquely identifies the current enocean interface" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="Status" Type="STRING" />
      <VarDeclaration Name="Message" Type="STRING" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="EnoceanCommInterface" />
</FBType>