<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BM_CAB" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Runtime.FBType.WeakImplementingType" Value="Runtime.Fieldbus#BM_SIMBUS" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2008-11-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SIMULATION" />
      </Event>
      <Event Name="CONFIG" Comment="Configure the devices on the bus">
        <With Var="QI" />
      </Event>
      <Event Name="SET_STATE" Comment="Set the master to a dedicated state">
        <With Var="STATE" />
      </Event>
      <Event Name="REQ" Comment="Bus cycle request">
        <With Var="QI" />
      </Event>
      <Event Name="RESTART" Comment="Reset the bus and restart">
        <With Var="QI" />
        <With Var="STATE" />
      </Event>
      <Event Name="ERROR_ACK" Comment="Error acknowledgement request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CONFIGO" Comment="Bus configuration confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_TIME" />
      </Event>
      <Event Name="CNF" Comment="Bus cycle request confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="STATEO" />
      </Event>
      <Event Name="ERROR_IND" Comment="Error indication">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="STATEO" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_TIME" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Uniquely identifies a bus in the device" />
      <VarDeclaration Name="SIMULATION" Type="BOOL" Comment="Puts the bus into simulation mode" />
      <VarDeclaration Name="STATE" Type="UDINT" Comment="Requested state (1 .. INIT, 2 .. PREOP, 4 .. SAFEOP, 8 .. OP)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays the current status of the bus" />
      <VarDeclaration Name="STATEO" Type="UDINT" Comment="Current master state" />
      <VarDeclaration Name="ERROR_CODE" Type="UDINT" Comment="Error code of the bus" />
      <VarDeclaration Name="ERROR_TIME" Type="DATE_AND_TIME" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>