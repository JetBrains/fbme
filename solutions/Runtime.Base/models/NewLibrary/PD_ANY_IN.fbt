<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="2BEBD4B222B673AC" Name="PD_ANY_IN" Comment="Service Interface Function Block Type" Namespace="Runtime.IoCommon">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.IoCommon#DYNGEN_PD_ANY_IN" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="1.0" Author="oberwal" Date="2008-11-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;1;16" />
    <EventInputs>
      <Event Name="INIT" Comment="Service initialization">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SLOT_NO" />
        <With Var="OFFSET" />
        <With Var="PD_USES_MSBF" />
        <With Var="IND_ON_CHG" />
        <With Var="IND_FACTOR" />
        <With Var="IND_OFFSET" />
      </Event>
      <Event Name="REQ" Comment="Process memory read request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirmation event">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Confirmation of a process memory read request">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${I}" />
      </Event>
      <Event Name="IND" Comment="Indication of a process memory change">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${I}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Bus identifier" />
      <VarDeclaration Name="SLOT_NO" Type="UDINT" Comment="Defines the device on the bus where values are read from or written to" />
      <VarDeclaration Name="OFFSET" Type="UINT" Comment="Defines the bit offset of the accessor within the device" />
      <VarDeclaration Name="PD_USES_MSBF" Type="BOOL" Comment="Defines process data to be encoded most significant byte first" />
      <VarDeclaration Name="IND_ON_CHG" Type="BOOL" Comment="TRUE: IND fired only on bit value change, FALSE: IND fired on every IND_FACTOR buscylce" />
      <VarDeclaration Name="IND_FACTOR" Type="UDINT" Comment="IND fired on every IND_FACTOR bus cycle (valid range 1..n)" />
      <VarDeclaration Name="IND_OFFSET" Type="UDINT" Comment="Starting value of IND count (valid range 1..IND_FACTOR, default: based on SLOT_NO)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays current accessor status" />
      <VarDeclaration Name="RD${I}" Type="&lt;BOOL, BYTE, WORD, DWORD, LWORD, REAL, LREAL&gt;" Comment="Data read from the associated field bus" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="PD_ANY_IN" />
</FBType>