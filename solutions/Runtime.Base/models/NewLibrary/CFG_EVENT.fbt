<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="CFG_EVENT" Comment="Bus device configuration events" Namespace="Runtime.Fieldbus">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Fieldbus#DYNGEN_CFG_EVENT" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="1.0" Author="oberwal" Date="2008-11-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;1;16" />
    <EventInputs>
      <Event Name="INIT" Comment="Service initialization">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SLOT_NO" />
        <With Var="OFFSETCNF" />
        <With Var="OFFSETREQ" />
      </Event>
      <Event Name="REQ${I}" Comment="Request a configuration event">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Service initialization confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF${I}" Comment="Confirmation that the configuration event was accepted by the function block">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="IND${I}" Comment="Indication that the event was processed by the specific device">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Bus identifier" />
      <VarDeclaration Name="SLOT_NO" Type="UDINT" Comment="Defines the device on the bus where the configuration is updated" />
      <VarDeclaration Name="OFFSETREQ" Type="UINT" Comment="Defines the bit offset of the accessor within the device for configuration request events. Each event needs a byte here." />
      <VarDeclaration Name="OFFSETCNF" Type="UINT" Comment="Defines the bit offset of the accessor within the device for configuration configrmation events. Each event needs a byte here." />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays current accessor status" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="CFG_EVENT" />
</FBType>