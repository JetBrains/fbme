<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="SNTPCLIENT" Comment="Basic Function Block Type" Namespace="Runtime.System">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Management" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="medusta" Date="19.06.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization Request">
        <With Var="QI" />
        <With Var="SERVERS" />
        <With Var="INTERVAL" />
        <With Var="TIMEOUT" />
        <With Var="INITRETRIES" />
        <With Var="MAXDIFFTOSYNC" />
      </Event>
      <Event Name="REQ" Comment="Force the synchronization" />
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization Confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="LASTDIFF" />
      </Event>
      <Event Name="CNF" Comment="Execution Confirmation">
        <With Var="STATUS" />
        <With Var="LASTDIFF" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" InitialValue="TRUE" Comment="Input event qualifier" />
      <VarDeclaration Name="SERVERS" Type="STRING" Comment="Comma-delimited list of server:port entries to try" />
      <VarDeclaration Name="INTERVAL" Type="UINT" InitialValue="15" Comment="Query interval in minutes, 0 to only sync on REQ event" />
      <VarDeclaration Name="TIMEOUT" Type="UINT" InitialValue="5" Comment="Timeout in seconds for the time server replies" />
      <VarDeclaration Name="INITRETRIES" Type="UINT" InitialValue="6" Comment="Number of retries of initial time sync" />
      <VarDeclaration Name="MAXDIFFTOSYNC" Type="UDINT" InitialValue="60000" Comment="Max difference in ms to sync (ignored at INIT sync)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Status of the last sync attempt" />
      <VarDeclaration Name="LASTDIFF" Type="DINT" Comment="Time difference in ms at last sync" />
    </OutputVars>
  </InterfaceList>
</FBType>