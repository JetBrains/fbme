<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="BM_ETHERCAT" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.IoCommon" />
  <Attribute Name="Runtime.FBType.WeakImplementingType" Value="Runtime.Fieldbus#BM_SIMBUS" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="2008-11-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Other" />
  </CompilerInfo>
  <InterfaceList>
    <EventInputs>
      <Event Name="INIT" Comment="Initialization request">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SIMULATION" />
      </Event>
      <Event Name="CONFIG" Comment="Configure the devices on the bus">
        <With Var="QI" />
      </Event>
      <Event Name="SET_STATE" Comment="Set a dedicated EtherCAT state">
        <With Var="STATE" />
        <With Var="STARTUP_MODE" />
      </Event>
      <Event Name="REQ" Comment="Bus cycle request">
        <With Var="QI" />
      </Event>
      <Event Name="RESTART" Comment="Reset the bus and restart">
        <With Var="QI" />
        <With Var="STATE" />
      </Event>
      <Event Name="CMD_SET">
        <With Var="CMD" />
        <With Var="CMD_ADP" />
        <With Var="CMD_ADO" />
        <With Var="CMD_DATA" />
      </Event>
      <Event Name="CMD_REQ" />
      <Event Name="START_SCAN" />
      <Event Name="SCAN_QUERY">
        <With Var="SCAN_SEL_ITEM" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Initialization confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CONFIGO" Comment="Bus configuration confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
      </Event>
      <Event Name="CNF" Comment="Bus cycle request confirm">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="ERROR_CODE" />
        <With Var="ERROR_ARG" />
        <With Var="CYCLE_CNT" />
        <With Var="CYCLE_TIME" />
        <With Var="STATEO" />
        <With Var="ECS_COUNT" />
        <With Var="ECS_WC" />
        <With Var="ECS_STATES" />
        <With Var="HC_STATE" />
        <With Var="HC_ERROR" />
      </Event>
      <Event Name="CMD_RSP">
        <With Var="RSP" />
        <With Var="RSP_ADP" />
        <With Var="RSP_ADO" />
        <With Var="RSP_DATA" />
        <With Var="RSP_WC" />
        <With Var="RSP_CNT" />
      </Event>
      <Event Name="SCAN_RSP">
        <With Var="SCAN_STATE" />
        <With Var="SCAN_CNT" />
      </Event>
      <Event Name="SCAN_ITEM_IND" Comment="will fires on every Slave property as a result of a Scan (SCAN START) ">
        <With Var="SCAN_ITEM_DATA" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Uniquely identifies a bus in a device" />
      <VarDeclaration Name="SIMULATION" Type="BOOL" Comment="Puts the bus into simulation mode" />
      <VarDeclaration Name="STATE" Type="UDINT" Comment="Requested Ethercat state (1 .. INIT, 2 .. PREOP, 4 .. SAFEOP, 8 .. OP)" />
      <VarDeclaration Name="CMD" Type="WORD" Comment="EC Command (see Ethercat technical referenz)" />
      <VarDeclaration Name="CMD_ADP" Type="INT" Comment="EC Slave Address depending on EC Command" />
      <VarDeclaration Name="CMD_ADO" Type="UINT" Comment="EC Slave Physical Memory(Register) offset" />
      <VarDeclaration Name="CMD_DATA" Type="STRING" Comment="Data in ASCII Hex (Stringlen / 2 = Datalen)" />
      <VarDeclaration Name="STARTUP_MODE" Type="UINT" Comment="0=COLDSTART (Autoincrement Addr), 1=Coldstart (store alias), 2=HOTCONNECT (warmstart)" />
      <VarDeclaration Name="SCAN_SEL_ITEM" Type="UINT" Comment="0=SCAN(for Devices),1=SCAN_AUTO_ALIAS(Store Alias in SII EEProm)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays the current status of the bus" />
      <VarDeclaration Name="ERROR_CODE" Type="DINT" Comment="Error code of the bus" />
      <VarDeclaration Name="ERROR_ARG" Type="DINT" Comment="Additional error information" />
      <VarDeclaration Name="CYCLE_CNT" Type="UDINT" Comment="The number of cycles the bus has executed" />
      <VarDeclaration Name="CYCLE_TIME" Type="UDINT" Comment="Time of the last read/write cycle of the bus" />
      <VarDeclaration Name="STATEO" Type="UDINT" Comment="Current Ethercat state" />
      <VarDeclaration Name="ECS_COUNT" Type="UDINT" Comment="Ethercat Slave (ECS) count in Configuration" />
      <VarDeclaration Name="ECS_WC" Type="UDINT" Comment="Ethercat Slave (ECS) Working Count (active slaves)" />
      <VarDeclaration Name="ECS_STATES" Type="WORD" Comment="Ethercat Slave (ECS) ORed states of all active slaves" />
      <VarDeclaration Name="RSP" Type="DINT" Comment="EC Command Result (0 = OK, otherwise error result)" />
      <VarDeclaration Name="RSP_ADP" Type="INT" Comment="EC Address result (may be incremented by ECS)" />
      <VarDeclaration Name="RSP_ADO" Type="UINT" Comment="the same as give by the CMD_ADO" />
      <VarDeclaration Name="RSP_DATA" Type="STRING" Comment="Data received from ECS" />
      <VarDeclaration Name="RSP_WC" Type="UINT" Comment="Working Count result processed by ECS" />
      <VarDeclaration Name="RSP_CNT" Type="SINT" Comment="incremented by every RSP received" />
      <VarDeclaration Name="HC_STATE" Type="UDINT" Comment="HotConnect State" />
      <VarDeclaration Name="HC_ERROR" Type="WORD" Comment="HotConnect Error Result" />
      <VarDeclaration Name="SCAN_STATE" Type="WORD" />
      <VarDeclaration Name="SCAN_CNT" Type="UINT" />
      <VarDeclaration Name="SCAN_ITEM_DATA" Type="STRING" Comment="Item property of a ECS (SCAN)" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" />
</FBType>