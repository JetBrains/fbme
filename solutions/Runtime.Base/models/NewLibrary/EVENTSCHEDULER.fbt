<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType GUID="68346A3AE15D763D" Name="EVENTSCHEDULER" Comment="Event schedule function block" Namespace="Runtime.Standard">
  <Attribute Name="ObsoleteNamespace" Value="nxtControl.Common" />
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.Standard#DYNGEN_EVENTSCHED" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH" Version="1.0" Author="oberwal" Date="09.09.2008" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;1;64" />
    <EventInputs>
      <Event Name="INIT" Comment="Initialization event">
        <With Var="QI" />
        <With Var="SCHEDULE" />
        <With Var="FIRSTCHANNEL" />
        <With Var="MAXINITTIME" />
      </Event>
      <Event Name="SET" Comment="Set new schedule event">
        <With Var="SCHEDULE" />
        <With Var="MAXINITTIME" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Init done notification">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="CHANNEL${I}" />
      </Event>
      <Event Name="SETO" Comment="Set new schedule done notification">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="CHANNEL${I}" />
      </Event>
      <Event Name="IND" Comment="Output state changed indicator">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="CHANNEL${I}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Initialize event input qualifier" />
      <VarDeclaration Name="FIRSTCHANNEL" Type="UDINT" Comment="Defines the number of the first valid channel which corresponds to CHANNEL0" />
      <VarDeclaration Name="MAXINITTIME" Type="TIME" Comment="Defines the timespan to go back to look for initialization events" />
      <VarDeclaration Name="SCHEDULE" Type="STRING" Comment="Defines a list of schedulable events (min hr day mon wday year ch val; ...)" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event status qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Error information" />
      <VarDeclaration Name="CHANNEL${I}" Type="DINT" Comment="Outputs the value for the channel given by FIRSTCHANNEL+i" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="EVENTSCHEDULER" />
</FBType>