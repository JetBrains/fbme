<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE FBType SYSTEM "../LibraryElement.dtd">
<FBType Name="CFG_DIRECT_GET" Comment="Service Interface Function Block Type" Namespace="Runtime.Fieldbus">
  <Attribute Name="Runtime.FBType.ImplementingType" Value="Runtime.IoCommon#DYNGEN_PD_DIRECT_IN" />
  <Attribute Name="Configuration.FB.IDCounter" Value="0" />
  <Attribute Name="nxtLibraryData" Value="" />
  <Identification Standard="61499-2" />
  <VersionInfo Organization="nxtControl GmbH " Version="1.0" Author="medusta" Date="2012-05-08" Remarks="Initial Version" />
  <CompilerInfo>
    <Compiler Language="Cpp" />
  </CompilerInfo>
  <InterfaceList>
    <Attribute Name="Configuration.GenericFBType.Counters" Value="I;1;1;32" />
    <EventInputs>
      <Event Name="INIT" Comment="Service initialization">
        <With Var="QI" />
        <With Var="BUS_ID" />
        <With Var="SLOT_NO" />
        <With Var="CFG_USES_MSBF" />
        <With Var="ADDR${I}" />
      </Event>
      <Event Name="REQ" Comment="Configuration memory read request">
        <With Var="QI" />
      </Event>
    </EventInputs>
    <EventOutputs>
      <Event Name="INITO" Comment="Service initialization done notification">
        <With Var="QO" />
        <With Var="STATUS" />
      </Event>
      <Event Name="CNF" Comment="Configuration memory read request confirmation">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${I}" />
      </Event>
      <Event Name="IND" Comment="Indication of a configuration memory change">
        <With Var="QO" />
        <With Var="STATUS" />
        <With Var="RD${I}" />
      </Event>
    </EventOutputs>
    <InputVars>
      <VarDeclaration Name="QI" Type="BOOL" Comment="Input event qualifier" />
      <VarDeclaration Name="BUS_ID" Type="STRING" Comment="Bus identifier" />
      <VarDeclaration Name="SLOT_NO" Type="UDINT" Comment="Defines the device on the bus where values are read from or written to" />
      <VarDeclaration Name="CFG_USES_MSBF" Type="BOOL" Comment="Defines configuration data to be encoded most significant byte first" />
      <VarDeclaration Name="ADDR${I}" Type="STRING" Comment="Address within the associated device ([%|!](I|Q)(X|B|W|D)addr[.bit])" />
    </InputVars>
    <OutputVars>
      <VarDeclaration Name="QO" Type="BOOL" Comment="Output event qualifier" />
      <VarDeclaration Name="STATUS" Type="STRING" Comment="Displays current accessor status" />
      <VarDeclaration Name="RD${I}" Type="&lt;BOOL, BYTE, WORD, DWORD&gt;" Comment="Data read from the given address in the associated device" />
    </OutputVars>
  </InterfaceList>
  <Service RightInterface="RESOURCE" LeftInterface="CFG_DIRECT_GET" />
</FBType>